    "rimraf": "^3.0.1",
    "stat-mode": "^1.0.0",
    "thunkify": "^2.1.2",
    "unyield": "0.0.1",
    "ware": "^1.2.0",
    "win-fork": "^1.1.1"
  },
  "engines": {
